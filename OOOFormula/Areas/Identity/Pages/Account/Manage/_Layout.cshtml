@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        //Layout = (string)parentLayout;
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}
<link rel="stylesheet" href="~/css/site.css" />
<h2 class="mt-2">Панель администратора</h2>

<nav class="navbar navbar-expand-lg navbar-dark menu">
    <div class="container">
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="navbar-collapse collapse ">
            <ul class="navbar-nav mr-auto col-md-3 col-lg-9">
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="Identity" asp-page="/Account/Manage">Профиль</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="" asp-page="/Account/">Каталог</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="" asp-page="">Галерея работ</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="" asp-page="">Доставка и оплата</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="" asp-page="">Контакты</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link nav-link-hover text-light" asp-area="" asp-page="">О нас</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<div>
    <hr />
    <div class="row">
        @RenderBody()
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
